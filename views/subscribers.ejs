<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Phone/Mobile</th>
      </tr>
    </thead>
    <tbody>
      <% subscribers.forEach(c => { %>
      <tr>
        <td><%= c.name %></td>
        <td><%= c.email %></td>
        <td><%= c.telNumber %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
